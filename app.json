{
  "expo": {
    "name": "DP_nutrisync_client",
    "slug": "DP_nutrisync_client",
    "version": "1.0.0",
    "scheme": [
      "dp-nutrisync-client"
    ],
    "newArchEnabled": true,
    "notification": {
      "icon": "./assets/icons/adaptive-icon.png",
      "color": "#ffffff",
      "androidMode": "default",
      "androidCollapsedTitle": "NutriSync",
      "iosDisplayInForeground": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "monochromeImage": "./assets/icons/adaptive-icon.png"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.syncsoft.nutrisync",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.health.READ_STEPS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-icon-light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "dark": {
            "image": "./assets/icons/splash-icon-dark.png"
          }
        }
      ],
      "expo-sqlite",
      "expo-health-connect",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 26
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./assets/icons/adaptive-icon.png",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "62eda9ea-be8d-4c07-bb64-a75a17cf0671"
      }
    }
  }
}
